{
  "name": "xueron/pails",
  "type": "library",
  "description": "Pails (PhalconRails) is used to create a rails-like PHP app based on Phalcon.",
  "keywords": [
    "pails",
    "phalcon",
    "rails",
    "phinx",
    "php",
    "phalcon-rails"
  ],
  "homepage": "https://pails.xueron.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Xueron Nee",
      "email": "xueron@xueron.com",
      "homepage": "https://pails.xueron.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-phalcon": "^3.0.0",
    "aliyuncs/oss-sdk-php": "^2.2",
    "doctrine/inflector": "^1.1",
    "guzzlehttp/guzzle": "^6.2",
    "league/flysystem": "^1.0",
    "league/fractal": "^0.14",
    "robmorgan/phinx": "^0.6",
    "symfony/console": "^3.2",
    "symfony/debug": "^3.2",
    "symfony/process": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^3.7|^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Pails\\": "src/Pails"
    },
    "files": [
      "src/Pails/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Pails\\": "tests/Pails"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
